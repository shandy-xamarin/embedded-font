<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ProduceReferenceAssembly>true</ProduceReferenceAssembly>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\Fonts\UTM Avo.ttf" />
    <None Remove="Resources\Fonts\UTM AvoBold.ttf" />
    <None Remove="Resources\Fonts\UTM AvoBold_Italic.ttf" />
    <None Remove="Resources\Fonts\UTM AvoItalic.ttf" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\Fonts\UTM Avo.ttf" />
    <EmbeddedResource Include="Resources\Fonts\UTM AvoBold.ttf" />
    <EmbeddedResource Include="Resources\Fonts\UTM AvoBold_Italic.ttf" />
    <EmbeddedResource Include="Resources\Fonts\UTM AvoItalic.ttf" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Xamarin.Forms" Version="4.6.0.1141" />  
    <PackageReference Include="Xamarin.Essentials" Version="1.5.3.2" />
  </ItemGroup>
</Project>